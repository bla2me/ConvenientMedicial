<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="#20AF65" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/top"
        android:background="#FFFFFF"
        android:orientation="vertical" >

        <ImageView
            android:id="@+id/IM_divider_horizontal_line"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="15dp"
            android:scaleType="fitXY"
            android:src="@drawable/divider_horizontal_line" />

        <ImageView
            android:id="@+id/IM_bg_header_top"
            android:layout_width="2dip"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/IM_divider_horizontal_line1"
            android:layout_below="@+id/Tv_Registered_information"
            android:layout_marginLeft="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/bg_header_top" />

        <ImageView
            android:id="@+id/IM_bg_header_top1"
            android:layout_width="2dip"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/IM_divider_horizontal_line1"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/Tv_Registered_information"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/bg_header_top" />

        <ImageButton
            android:id="@+id/Ib_back"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_alignParentLeft="true"
            android:layout_alignTop="@+id/Tv_Payment_details"
            android:layout_below="@+id/IM_divider_horizontal_line"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="4dp"
            android:background="@null"
            android:scaleType="fitXY"
            android:src="@drawable/leftarrow" />

        <TextView
            android:id="@+id/Tv_Payment_details"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/IM_divider_horizontal_line"
            android:layout_marginLeft="45dp"
            android:layout_marginRight="45dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="@string/Payment_details"
            android:textColor="#323232"
            android:textSize="22sp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/IM_line_x"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/Tv_Payment_details"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_marginTop="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/line_x" />

        <TextView
            android:id="@+id/Tv_Payment_item"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/IM_line_x"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="150dp"
            android:layout_marginTop="10dp"
            android:gravity="left"
            android:text="@string/Payment_item"
            android:textColor="#4B4B4B"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Payment_item_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/IM_line_x"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:text="@string/Payment_item_result"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Payment_category"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Tv_Payment_item_result"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="150dp"
            android:layout_marginTop="10dp"
            android:gravity="left"
            android:text="@string/Payment_category"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Payment_category_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/Tv_Payment_item_result"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:text="@string/Payment_category_result"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Amount_of_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Tv_Payment_category_result"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="150dp"
            android:layout_marginTop="10dp"
            android:gravity="left"
            android:text="@string/Amount_of_money"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Amount_of_money_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/Tv_Payment_category_result"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:text="@string/Amount_of_money_result"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Medical_insurance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/Tv_Amount_of_money_result"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:text="@string/Medical_insurance"
            android:textColor="#4b4b4b"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/Tv_Paid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/Tv_Medical_insurance"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="150dp"
            android:layout_marginTop="10dp"
            android:gravity="left"
            android:text="@string/Paid"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/Tv_Paid_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/Tv_Medical_insurance"
            android:layout_marginRight="30dp"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:text="@string/Paid_result"
            android:textColor="#4b4b4b"
            android:textSize="22sp" />

        <Button
            android:id="@+id/BT_To_give_up"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_below="@+id/Tv_Paid_result"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/editsharp"
            android:gravity="center"
            android:text="@string/To_give_up"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <Button
            android:id="@+id/BT_Pay"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_alignRight="@+id/IM_bg_header_top1"
            android:layout_below="@+id/Tv_Paid_result"
            android:layout_marginRight="40dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/editsharp1"
            android:gravity="center"
            android:text="@string/Pay"
            android:textColor="#FFFFFF"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/IM_divider_horizontal_line1"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/BT_Pay"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="15dp"
            android:scaleType="fitXY"
            android:src="@drawable/divider_horizontal_line" />
    </RelativeLayout>

</RelativeLayout>